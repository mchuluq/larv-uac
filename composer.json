{
    "name": "mchuluq/larv-uac",
    "description": "A Laravel User Access Control.",
    "keywords": [
        "laravel",
        "session",
        "rbac",
        "uac"
    ],
    "support": {
        "issues": "https://github.com/mchuluq/larv-uac/issues",
        "source": "https://github.com/mchuluq/larv-uac"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mochammad C. Chuluq",
            "email": "mchuluq@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "doctrine/dbal": "^2.10",
        "webpatser/laravel-uuid": "3.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Mchuluq\\Laravel\\Uac\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mchuluq\\Laravel\\Uac\\UacServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
